<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_choose_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.felicelin.photodiary.ChooseLayout">


    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/layout_blank"
        android:id="@+id/Layout_4"
        android:layout_alignTop="@+id/Layout_3"
        android:layout_alignLeft="@+id/Layout_2"
        android:layout_alignStart="@+id/Layout_2"
        android:background="@android:color/transparent"  />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/layout_blank"
        android:id="@+id/Layout_3"
        android:layout_marginTop="54dp"
        android:background="@android:color/transparent"
        android:layout_below="@+id/Layout_1"
        android:layout_alignLeft="@+id/Layout_1"
        android:layout_alignStart="@+id/Layout_1" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/layout_blank"
        android:id="@+id/Layout_2"
        android:background="@android:color/transparent"
        android:layout_alignTop="@+id/Layout_1"
        android:layout_toRightOf="@+id/Layout_1"
        android:layout_toEndOf="@+id/Layout_1"
        android:layout_marginLeft="42dp"
        android:layout_marginStart="42dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/layout_blank"
        android:id="@+id/Layout_1"
        android:layout_marginTop="38dp"
        android:background="@android:color/transparent"
        android:layout_marginLeft="25dp"
        android:layout_marginStart="25dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
</RelativeLayout>
