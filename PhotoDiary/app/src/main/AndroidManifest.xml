<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.felicelin.photodiary">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ViewEventList" />
        <activity android:name=".ChooseLayout" />
        <activity android:name=".Layout1" android:windowSoftInputMode="adjustResize"/>
        <activity android:name=".Layout2" android:windowSoftInputMode="adjustResize"/>
        <activity android:name=".Layout3" android:windowSoftInputMode="adjustResize"/>
        <activity android:name=".Layout4" android:windowSoftInputMode="adjustResize"/>
        <activity android:name=".ViewLayout1" />
        <activity android:name=".ViewLayout2" />
        <activity android:name=".ViewLayout3" />
        <activity android:name=".ViewLayout4"></activity>
        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>